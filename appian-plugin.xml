<appian-plugin name="AW Advanced Plugins" key="com.appiancorp.plugins.appianworld.advancedplugins">

  <plugin-info>
    <description>AW Advanced Plugins</description>
    <vendor name="Appian Corporation" url="http://www.appian.com" />
    <version>1.0.11</version>
    <application-version min="7.1.0" />
  </plugin-info>

   <!-- Declare the datatype Address based on the POJO AddressDataType -->  
  <datatype key="AW_AddressDataType" name="Address for customer for AW">
    <class>com.appiancorp.plugins.appianworld.advancedplugins.datatypes.AddressDataType</class>
  </datatype>

  <!-- Declare the datatype Customer based on the POJO CustomerDataType -->  
  <datatype key="AW_CustomerDataType" name="Customer for AW">
    <class>com.appiancorp.plugins.appianworld.advancedplugins.datatypes.CustomerDataType</class>
  </datatype>


    
</appian-plugin>
